    "installCommand": "npm install",
    "framework": "nextjs",
    "regions": [
        "iad1"
    ],
    "functions": {
        "app/api/**": {
            "maxDuration": 30
        }
    },
    "env": {
        "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
    }
}